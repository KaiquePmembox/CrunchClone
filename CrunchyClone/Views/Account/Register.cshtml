<h2>Cadastro</h2>
<form asp-action="Register" method="post">
    <div>
        <label>Usuário</label>
        <input type="text" name="Username" required />
    </div>
    <div>
        <label>Email</label>
        <input type="email" name="Email" required />
    </div>
    <div>
        <label>Senha</label>
        <input type="password" name="Password" required />
    </div>
    <button type="submit">Cadastrar</button>
</form>
